{
    "plugins": [
        "@typescript-eslint"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "@typescript-eslint/parser"
    },
    "env": {
        "node": true
    },
    "ignorePatterns": ["node_modules/", "dist/", "*.d.ts"],
    "extends": [
        "eslint:recommended",
        "plugin:vue/base"
    ],
    "rules": {
        "no-unused-vars": "off"
    }
}
